{
  "name": "@uju-labs/infinity-swap-core",
  "version": "0.1.0",
  "description": "The official types package for the Infinity Swap protocol",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "schema",
    "artifacts",
    "dist"
  ],
  "scripts": {
    "build": "bun run dist",
    "codegen": "./scripts/schema.sh",
    "dist": "tsup index.ts --format cjs,esm --dts",
    "lint": "eslint --fix .",
    "optimize": "./scripts/optimize.sh",
    "optimize-arm": "./scripts/optimize-arm.sh",
    "changeset": "changeset add",
    "test:unit": "cargo test --lib -- --nocapture",
    "wasm": "cargo build --release --lib --target wasm32-unknown-unknown"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@uju-labs/eslint-config-custom": "workspace:*",
    "tsup": "^7.3.0"
  }
}
