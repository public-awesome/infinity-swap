{
  "name": "@uju-labs/infinity-swap",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "files": [
    "schema",
    "artifacts",
    "dist"
  ],
  "scripts": {
    "codegen": "./scripts/schema.sh",
    "lint": "cargo clippy --all-targets -- -D warnings && eslint .",
    "wasm": "cargo build --release --lib --target wasm32-unknown-unknown",
    "build": "bun run wasm && tsc -p ./tsconfig.json",
    "optimize": "./scripts/optimize.sh",
    "optimize-arm": "./scripts/optimize-arm.sh",
    "test:unit": "cargo test --lib -- --nocapture"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "typescript": "5.1.6",
    "eslint": "^8.48.0"
  },
  "devDependencies": {
    "@uju-labs/eslint-config-custom": "workspace:*"
  }
}
